{
  "name": "frontend-angular",
  "description": "bootstrap/angular seed project based on angular-cli scaffolding",
  "author": "Mike Echevarria <mechevar@redhat.com>",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mechevarria/frontend-angular.git"
  },
  "ng": "node_modules/@angular/cli/bin/ng",
  "scripts": {
    "ng": "$npm_package_ng",
    "local": "$npm_package_ng serve --host 0.0.0.0 --proxy-config proxy.json",
    "build": "$npm_package_ng build --delete-output-path --prod",
    "buildDev": "$npm_package_ng build --watch",
    "lint": "$npm_package_ng lint",
    "start": "node server.js proxy.json",
    "startDev": "node server.js proxy.json 4200"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^0.7.0",
    "@angular/animations": "~6.1.0",
    "@angular/cli": "~6.1.0",
    "@angular/common": "~6.1.0",
    "@angular/compiler": "~6.1.0",
    "@angular/compiler-cli": "~6.1.0",
    "@angular/core": "~6.1.0",
    "@angular/forms": "~6.1.0",
    "@angular/http": "~6.1.0",
    "@angular/language-service": "~6.1.0",
    "@angular/platform-browser": "~6.1.0",
    "@angular/platform-browser-dynamic": "~6.1.0",
    "@angular/router": "~6.1.0",
    "@exalif/ngx-breadcrumbs": "^6.0.1",
    "@fortawesome/angular-fontawesome": "^0.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@types/node": "~10.5.3",
    "bootstrap": "^4.1.3",
    "bootswatch": "^4.1.3",
    "chart.js": "^2.7.2",
    "codelyzer": "^4.4.2",
    "compression": "^1.7.3",
    "core-js": "^2.5.7",
    "express": "~4.16.0",
    "http-proxy-middleware": "^0.18.0",
    "morgan": "~1.9.0",
    "ng2-charts": "^1.6.0",
    "ngx-bootstrap": "~3.0.1",
    "ngx-toastr": "^8.10.0",
    "npm-sort": "0.0.4",
    "rxjs": "~6.2.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "zone.js": "^0.8.26"
  }
}
